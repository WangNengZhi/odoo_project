<odoo>

    <record id="fsn_base_category" model="ir.module.category">
        <field name="name">FSN基础群组</field>
        <field name="sequence">5</field>
    </record>
        <record id="fsn_super_user_group" model="res.groups">
            <field name="name">超级用户</field>
            <field name="category_id" ref="fsn_base_category"/>
        </record>
        <record id="fsn_executives_group" model="res.groups">
            <field name="name">高级管理</field>
            <field name="category_id" ref="fsn_base_category"/>
        </record>
        <record id="fsn_insiders_group" model="res.groups">
            <field name="name">内部人员</field>
            <field name="category_id" ref="fsn_base_category"/>
        </record>
        <record id="fsn_base_read_group" model="res.groups">
            <field name="name">基础信息（读）</field>
            <field name="category_id" ref="fsn_base_category"/>
        </record>
        <record id="fsn_base_write_group" model="res.groups">
            <field name="name">基础信息（写）</field>
            <field name="category_id" ref="fsn_base_category"/>
        </record>

</odoo>