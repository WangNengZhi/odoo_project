                                    <th scope="col">人均产值</th>
                                </tr>
                            </thead>
                            <tbody id="fsn_group_avg_output">

                            </tbody>
                        </table>
                    </div>

                </div>

                <div class="row">
                    <div class="col" style="margin-top: 30px;">
                        <!-- <h2 style="text-align: center;"></h2> -->
                        <h2 style="text-align: center;"><span id="fsn_production_plan_progress_date"></span>生产计划进度</h2>
                        <!-- <div id="fsn_production_plan_progress">

                        </div> -->
                        <table id="fsn_production_plan_progress" class="table">
                            <thead>
                                <tr>
                                    <th scope="col" width="70px">组别</th>
                                    <th scope="col">进度</th>
                                </tr>
                            </thead>
                        </table>
                    </div>
                </div>
            </div>


        </div>
    </t>


</templates>